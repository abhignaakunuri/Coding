 public class Main {
    public static void main(String[] args) {

        int[] arr = {10,5,10};
        int s =arr[0];
        int ss =-1;
        for (int i = 0; i < arr.length ; i++) {
            if (arr[i] > s) {
                ss = s;
                s = arr[i];
            }
            else if (arr[i] < s && arr[i] > ss) {
                ss = arr[i];
            }
        }
            System.out.println(ss);
        }
    }
